reg clk;
reg clr;
reg en;
reg prn;
// wires                                               
wire m1;
wire m2;
wire m3;
wire rez;
wire rez_mura;
wire rez_y;
wire [1:0] s_true;
wire t1;
wire t2;
wire [6:0] testseq;
wire tt1;
wire tt2;
wire tt3;
wire x1;
wire x2;
wire y1;
wire y2;
wire y3;
wire [2:0] y_true;

wire sampler;                             

// assign statements (if any)                          
tstand i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.clr(clr),
	.en(en),
	.m1(m1),
	.m2(m2),
	.m3(m3),
	.prn(prn),
